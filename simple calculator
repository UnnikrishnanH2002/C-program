#include <stdio.h>

int main() 
{
    int a, b,c,n,e,d; // Declare variables to store input and results
    printf("Enter the case: ");
    scanf("%d %d %d", &a, &b, &n);
    switch(n)
    {
        case 1:
        c=a+b;
        printf("c=%d\n",c);
        break;
        case 2:
        d = a - b;
        printf("d=%d\n",d);
        break;
        case 3:
        e = a*b;
        printf("e=%d\n",e);
        break;
    return 0;
    }
    
}
